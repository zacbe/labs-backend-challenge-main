{
  "openapi": "3.0.0",
  "info": {
    "title": "Labs Challenge API",
    "description": "Labs Challenge API Documentation",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8081/"
    }
  ],
  "paths": {
    "/api/v1/orders": {
      "post": {
        "description": "This function creates a new order in the Database.",
        "responses": {
          "201": {
            "description": "Order created"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "description": {
                    "example": "This is a new order"
                  },
                  "userId": {
                    "example": "662fd536ea8c270e6fdadf65"
                  }
                }
              }
            }
          }
        }
      },
      "get": {
        "description": "This function retrieves all contracts from the Database.",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "description": {
                        "type": "string"
                      },
                      "orderId": {
                        "type": "string"
                      },
                      "user": {
                        "type": "object",
                        "properties": {
                          "firstName": {
                            "type": "string"
                          },
                          "lastName": {
                            "type": "string"
                          },
                          "streetAddress": {
                            "type": "string"
                          },
                          "houseNumber": {
                            "type": "string"
                          },
                          "zip": {
                            "type": "string"
                          },
                          "city": {
                            "type": "string"
                          },
                          "country": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/orders/{orderId}": {
      "put": {
        "description": "",
        "parameters": [
          {
            "name": "orderId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Order updated"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "description": {
                    "example": "This is a new order"
                  }
                }
              }
            }
          }
        }
      },
      "get": {
        "description": "This function retrieves a specific order from the Database.",
        "parameters": [
          {
            "name": "orderId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "description": {
                      "type": "string"
                    },
                    "orderId": {
                      "type": "string"
                    },
                    "user": {
                      "type": "object",
                      "properties": {
                        "firstName": {
                          "type": "string"
                        },
                        "lastName": {
                          "type": "string"
                        },
                        "streetAddress": {
                          "type": "string"
                        },
                        "houseNumber": {
                          "type": "string"
                        },
                        "zip": {
                          "type": "string"
                        },
                        "city": {
                          "type": "string"
                        },
                        "country": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "delete": {
        "description": "This function deletes a specific order from the Database.",
        "parameters": [
          {
            "name": "orderId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Order deleted"
          }
        }
      }
    }
  }
}